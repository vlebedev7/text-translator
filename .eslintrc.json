{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["google", "prettier"],
  "plugins": ["react", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "linebreak-style": 0,
    "require-jsdoc": "off",
    "no-unused-vars": [
      "warn",
      { "vars": "local", "args": "after-used", "ignoreRestSiblings": false }
    ]
  }
}
